{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "output": "dist",
        "routes": [
            {
              "src": "/api/.*",
              "dest": "src/index.ts"
            },
            {
              "src": "/auth/.*",
              "dest": "src/index.ts"
            },
            {
              "src": "/.*",
              "dest": "src/index.ts"
            }
          ]
      }
    ]
  }